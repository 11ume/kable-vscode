{
	"root": true,
	"parser": "@typescript-eslint/parser",
	"plugins": [
		"@typescript-eslint"
	],
	"extends": [
		"eslint:recommended",
		"plugin:@typescript-eslint/eslint-recommended",
		"plugin:@typescript-eslint/recommended"
	],
	"rules": {
		"quotes": [
			"error",
			"single"
		],
		"comma-style": [
			"error",
			"first"
		],
		"multiline": {
			"delimiter": "comma",
			"requireLast": true
		},
		"singleline": {
			"delimiter": "comma",
			"requireLast": true
		},
		"no-param-reassign": "error",
		"no-multiple-empty-lines": [
			"error",
			{
				"max": 2
			}
		]
	}
}